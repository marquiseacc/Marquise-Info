@model Marquise_Web.UI.areas.CRM.Models.ContractDetailVm

<div class="card h-100">
    <div class="card-body">
        <div class="row m-0 row-group text-center border border-light-3">
            <div class="col-12 col-lg-4">
                <div class="p-3">
                    <h5 class="mb-0">نوع پشتیبانی</h5>
                    <small class="mb-0">@Model.sodoureghoflName__C</small>
                </div>
            </div>
            <div class="col-12 col-lg-4">
                <div class="p-3">
                    <h5 class="mb-0">تاریخ ثبت</h5>
                    <small class="mb-0">@Model.CreateDate</small>
                </div>
            </div>
            <div class="col-12 col-lg-4">
                <div class="p-3">
                    <h5 class="mb-0">وضعیت</h5>
                    <small class="mb-0">فعال</small>
                </div>
            </div>
        </div>


        <h6 class="card-title pt-5 pb-2">مشخصات قرارداد</h6>
        <div class="row m-0 row-group text-center border border-light-3">
            <div class="col-12 col-lg-4">
                <div class="p-3">
                    <h5 class="mb-0">تاریخ شروع</h5>
                    <small class="mb-0">@Model.datestart__C</small>
                </div>
            </div>
            <div class="col-12 col-lg-4">
                <div class="p-3">
                    <h5 class="mb-0">تاریخ اتمام</h5>
                    <small class="mb-0">@Model.dateend__C</small>
                </div>
            </div>
            <div class="col-12 col-lg-4">
                <div class="p-3">
                    <h5 class="mb-0">وضعیت تمدید خودکار</h5>
                    <small class="mb-0">@Model.tamdidStatus</small>
                </div>
            </div>
        </div>
    </div>
</div>
